#include<iostream>
#include<cstdio>
#include<cmath>
#include<vector>
#include<string>
#include<algorithm>
	
#define ll long long
using namespace std;		
			

ll en(){
	ll n; cin >> n;
	if(n<0)
	n*=-1;
	return n;
}
	
int main() {	
	cin.tie(nullptr);
	cout.tie(nullptr);
	ios_base::sync_with_stdio(false);
	ll count = 0;
	ll n = en();
	for(int i = 1; i<=n; i++){
		ll form = pow((9*i), 3);
		count+=form;
	}
	cout << count;

	return 0;
}
